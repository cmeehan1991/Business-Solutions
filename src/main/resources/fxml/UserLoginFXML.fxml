<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="249.0" prefWidth="206.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cbmwebdevelopment.userLogin.UserLoginController">
   <children>
      <ImageView fitHeight="79.0" fitWidth="70.0" layoutX="71.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resources/web_icon.png" />
         </image>
      </ImageView>
      <TextField id="textField__square" fx:id="usernameTextField" layoutX="22.0" layoutY="89.0" onAction="#userSignIn" promptText="Username" />
      <PasswordField id="textField__square" fx:id="passwordTextField" layoutX="24.0" layoutY="128.0" onAction="#userSignIn" prefHeight="27.0" prefWidth="167.0" promptText="Password" />
      <Button id="signInButton" layoutX="24.0" layoutY="162.0" mnemonicParsing="false" onAction="#userSignIn" prefHeight="27.0" prefWidth="167.0" style="-fx-background-color: transparent; -fx-border-color: #000000; -fx-border-radius: 5px; -fx-cursor: pointer;" text="Sign In" />
      <Hyperlink layoutX="21.0" layoutY="199.0" onAction="#recoverUser" text="Forgot Login Credentials" />
      <Hyperlink layoutX="67.0" layoutY="224.0" onAction="#createNewUser" text="New User" />
   </children>
</AnchorPane>
